script/source = "extends Sprite2D

# Drag and drop variables
var is_dragging = false
var drag_offset = Vector2()
var original_position = Vector2()
var drop_zones = []

# Visual feedback
var original_modulate = Color.WHITE
var drag_modulate = Color(1, 1, 1, 0.7)  # Semi-transparent while dragging

# Signals for drag and drop events
signal drag_started(sprite)
signal drag_ended(sprite)
signal dropped_on_zone(sprite, zone)
signal dropped_on_invalid_area(sprite)

func _ready():
    # Store original position for potential snap-back
    original_position = global_position
    original_modulate = modulate
    
    # Create Area2D for mouse detection
    var area = Area2D.new()
    var collision = CollisionShape2D.new()
    var shape = RectangleShape2D.new()
    
    # Add as children
    add_child(area)
    area.add_child(collision)
    
    # Configure collision shape to match sprite
    if texture:
        shape.size = texture.get_size()
    else:
        shape.size = Vector2(64, 64)
    
    collision.shape = shape
    area.input_pickable = true
    area.monitoring = true
    area.monitorable = true
    
    # Connect signals
    area.connect(\"input_event\", _on_area_input_event)
    area.connect(\"mouse_entered\", _on_mouse_entered)
    area.connect(\"mouse_exited\", _on_mouse_exited)

func _on_area_input_event(viewport, event, shape_idx):
    if event is InputEventMouseButton and event.button_index == MOUSE_BUTTON_LEFT:
        if event.pressed:
            start_drag()
        else:
            end_drag()

func _on_mouse_entered():
    pass  # No hover effect

func _on_mouse_exited():
    pass  # No hover effect

func start_drag():
    is_dragging = true
    drag_offset = global_position - get_global_mouse_position()
    
    # Bring to front while dragging (no transparency effect)
    z_index = 100
    
    # Find all drop zones in the scene
    find_drop_zones()
    
    # Emit signal
    emit_signal(\"drag_started\", self)

func end_drag():
    if not is_dragging:
        return
    
    is_dragging = false
    
    # Reset z-index
    z_index = 0
    
    # Check if dropped on a valid zone
    var dropped_zone = check_drop_zones()
    
    if dropped_zone:
        # Successfully dropped on a zone
        handle_successful_drop(dropped_zone)
    else:
        # Dropped on invalid area
        handle_invalid_drop()
    
    # Emit signal
    emit_signal(\"drag_ended\", self)

func _process(delta):
    if is_dragging:
        global_position = get_global_mouse_position() + drag_offset

func find_drop_zones():
    # Find all nodes in the scene tagged as drop zones
    drop_zones.clear()
    var root = get_tree().current_scene
    _recursive_find_drop_zones(root)

func _recursive_find_drop_zones(node):
    # Look for nodes with \"drop_zone\" in groups or specific class
    if node.is_in_group(\"drop_zone\"):
        drop_zones.append(node)
    
    for child in node.get_children():
        _recursive_find_drop_zones(child)

func check_drop_zones():
    # Check if the sprite overlaps with any drop zone
    for zone in drop_zones:
        if zone.has_method(\"can_accept_drop\") and not zone.can_accept_drop(self):
            continue
            
        if is_overlapping_zone(zone):
            return zone
    return null

func is_overlapping_zone(zone):
    # Simple bounds checking - override for more complex shapes
    var sprite_rect = get_sprite_rect()
    var zone_rect = zone.get_zone_rect() if zone.has_method(\"get_zone_rect\") else get_node_rect(zone)
    
    return sprite_rect.intersects(zone_rect)

func get_sprite_rect():
    if texture:
        var size = texture.get_size()
        return Rect2(global_position - size/2, size)
    else:
        return Rect2(global_position - Vector2(32, 32), Vector2(64, 64))

func get_node_rect(node):
    # Get bounding rectangle for various node types
    if node is Sprite2D and node.texture:
        var size = node.texture.get_size()
        return Rect2(node.global_position - size/2, size)
    elif node is ColorRect:
        return Rect2(node.global_position, node.size)
    elif node.has_method(\"get_rect\"):
        var rect = node.get_rect()
        return Rect2(node.global_position + rect.position, rect.size)
    else:
        # Default size
        return Rect2(node.global_position - Vector2(50, 50), Vector2(100, 100))

func handle_successful_drop(zone):
    # Position sprite at the drop zone center (optional)
    if zone.has_method(\"get_drop_position\"):
        global_position = zone.get_drop_position()
    
    # Notify the drop zone
    if zone.has_method(\"on_item_dropped\"):
        zone.on_item_dropped(self)
    
    # Update original position so it doesn't snap back
    original_position = global_position
    
    emit_signal(\"dropped_on_zone\", self, zone)

func handle_invalid_drop():
    # Just emit signal, no snap back
    emit_signal(\"dropped_on_invalid_area\", self)

func snap_back_to_original():
    # Smooth animation back to original position
    var tween = create_tween()
    tween.tween_property(self, \"global_position\", original_position, 0.3)
    tween.tween_callback(func(): modulate = original_modulate)

# Public methods for external control
func set_draggable(draggable: bool):
    var area = get_child(0) as Area2D
    if area:
        area.input_pickable = draggable

func reset_to_original_position():
    global_position = original_position
    modulate = original_modulate
    is_dragging = false

func set_original_position(pos: Vector2):
    original_position = pos
"

